{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": [2, "always"],
    "no-unused-vars": "error",
    "require-await": "error",
    "@typescript-eslint/no-explicit-any": "error"
  },
  "ignorePatterns":[
    "*.config.js",
    "*.d.ts"
  ]
}